{
    "name": "gogilo/sms-parser-cli",
    "description": "A CLI tool to extract M-Pesa transactions from SMS XML backups",
    "type": "project",
    "license": "MIT",
    "bin": [
        "bin/parse-sms"
    ],
    "authors": [
        {
            "name": "Ogilo G. D. Ouma",
            "email": "gogilo2003@hotmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^12.1",
        "symfony/filesystem": "^7.2",
        "symfony/process": "^7.2"
    },
    "require": {
        "illuminate/console": "^12.15",
        "symfony/console": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Gogilo\\SmsParserCli\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gogilo\\SmsParserCli\\Console\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
